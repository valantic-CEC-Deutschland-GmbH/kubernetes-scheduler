{
  "name": "valantic-spryker/kubernetes-scheduler",
  "type": "library",
  "description": "Kubernetes Scheduler implementation",
  "homepage": "https://www.valantic.com",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "maclof/kubernetes-client": "^0.26.0",
    "spryker/kernel": "^3.56.0",
    "spryker/log": "^3.7.0",
    "spryker/queue": "^1.5.0",
    "spryker/symfony": "^3.5.0",
    "spryker/transfer": "^3.25.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "ValanticSpryker\\": "src/ValanticSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ValanticSprykerTest\\": "tests/ValanticSprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lowest": "validate-prefer-lowest",
    "lowest-setup": "composer update --prefer-lowest --prefer-stable --prefer-dist --no-interaction && cp composer.json composer.backup && composer require --dev dereuromark/composer-prefer-lowest && mv composer.backup composer.json"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "gitlab-domains": ["gitlab.nxs360.com"]
  },
  "authors": [
    {
      "name": "Valantic",
      "homepage": "https://www.valantic.com"
    }
  ],
  "keywords": [
    "kubernetes",
    "scheduler"
  ],
  "include-path": [
    "src/"
  ]
}
